version 18
__lua__
// mini project one mario
x = 64 y = 64
function _update()
	if (btn(0)) then x=x-1 end 
	if (btn(1)) then x=x+1 end
	if (btn(2)) then y=y-1 end
	if (btn(3)) then y=y+1 end 
end 

function _draw()
	cls()
	map(0,0,0,0,16,16)
	spr(6,x,y)
end



